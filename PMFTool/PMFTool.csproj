<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PMFToolVersion>1.0.0.2</PMFToolVersion>
		<PMFToolAuthors>XGames105</PMFToolAuthors>
	</PropertyGroup>
	
	
  <PropertyGroup>
	  <OutputType>Exe</OutputType>
	  <TargetFramework>net6.0</TargetFramework>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>
	  <PackAsTool>true</PackAsTool>
	  <ToolCommandName>PMFTool</ToolCommandName>
	  <PackageOutputPath>./bin/Release/nupkg</PackageOutputPath>

	  <Version>$(PMFToolVersion)</Version>
	  <AssemblyVersion>$(PMFToolVersion)</AssemblyVersion>
	  <FileVersion>$(PMFToolVersion)</FileVersion>


	  <Company>$(PMFToolAuthors)</Company>
	  <Authors>$(PMFToolAuthors)</Authors>
	  <AssemblyName>PMFTool</AssemblyName>
	  <PackageId>XGames105.PMFTool</PackageId>
	  <Description>A tool to help make mods using PMF</Description>
		

  </PropertyGroup>
	
	
  <ItemGroup>
    <PackageReference Include="Cocona.Lite" Version="2.0.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Sini" Version="1.0.5" />
  </ItemGroup>

</Project>
